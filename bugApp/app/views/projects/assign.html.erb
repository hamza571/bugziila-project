<p id="notice"><%= notice %></p>
<h1>Assinging Projects</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>User Type </th>
      <th>Assign</th>
      <th>Remove</th>
    </tr>
  </thead>
  <% @assigned_users.each do |t|%>
    <tr>
      <td><%= t.id %></td>
      <td><%= t.name %></td>
      <td><%= t.user_type %></td>
      <td><%= link_to 'Remove', user_project_remove_project_path(current_user.id, @project.id, t.id) %></td>

    </tr>
  <%end%>
  <% @remove_user.each do |t|%>
    <tr>
      <td><%= t.id %></td>
      <td><%= t.name %></td>
      <td><%= t.user_type %></td>
      <td><%= link_to 'Add', user_project_assign_project_path(current_user, @project.id, t.id)%></td>
    </tr> 
  <%end%>
</table>

<br>
